  "keywords": [
    "email",
    "validator",
    "check"
  ],
  "version": "1.0.0",
  "homepage": "https://github.com/ichinya/mail-validator",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Ichi",
      "email": "info@ichiblog.ru",
      "homepage": "https://ichiblog.ru",
      "role": "Developer"
    }
  ],
  "autoload": {
